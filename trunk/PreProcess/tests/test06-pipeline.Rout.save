> # simulate a moderately realistic looking microarray
> nc <- 100
> nr <- 100
> v <- rexp(nc*nr, 1/1000)
> b <- rnorm(nc*nr, 80, 10)
> s <- sapply(v-b, max, 1)
> ct <- ChannelType('user', 'random', nc, nr,  'fake')
> subbed <- Channel(name='fraud', parent='', type=ct, vec=s)
